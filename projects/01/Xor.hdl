/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=notA);
    Not(in=b,out=notB);
    And(a=a,b=notB,out=aNb);
    And(a=notA,b=b,out=bNa);
    Or(a=aNb,b=bNa,out=out);
}